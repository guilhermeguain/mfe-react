{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@/assets": "./src/assets",
          "@/components": "./src/components",
          "@/hooks": "./src/hooks",
          "@/lib": "./src/lib",
          "@/services": "./src/services",
          "@/styles": "./src/styles"
        }
      }
    ]
  ]
}
